import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Test {
	public static void main(String... args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int n = Integer.parseInt(br.readLine());
		char[] swapOut = new char[n];
		char[] swapIn = new char[n];
		StringTokenizer st;
		for (int i = 0; i < n; ++i) {
			st = new StringTokenizer(br.readLine());
			swapOut[i] = st.nextToken().charAt(0);
			swapIn[i] = st.nextToken().charAt(0);
		}
		String swapsO = new String(swapOut);
		String swapsI = new String(swapIn);
		char[] prog = br.readLine().toCharArray();
		int index;
		for(int i=0; i<prog.length; ++i) {
			index=swapsO.indexOf(prog[i]);
			if(index!=-1) {
				prog[i]=swapIn[index];
			}else {
				index= swapsI.indexOf(prog[i]);
			}
		} 
		System.out.println(new String(prog));
	}
}
